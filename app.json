{
    "name": "gromit-backend",
    "description": "Gromit Document Intelligence Engine Backend",
    "repository": "https://github.com/TheRealGalli/compilator",
    "env": {
        "NODE_ENV": {
            "description": "Ambiente di esecuzione",
            "value": "production"
        },
        "STORAGE_MODE": {
            "description": "Modalità di storage (gcs o local)",
            "value": "local"
        },
        "PRIVATE_CLOUD": {
            "description": "Identifica se l'istanza è un Private Cloud",
            "value": "true"
        }
    },
    "options": {
        "allow-unauthenticated": false
    }
}